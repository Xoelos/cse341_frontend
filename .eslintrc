{
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended"
  ],
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "no-console": 0,
    "import/newline-after-import": 0,
    "object-curly-newline": 0,
    "no-use-before-define": 0,
    "no-restricted-globals": 0,
    "no-shadow": 0,
    "no-param-reassign": 0,
    "no-throw-literal": 0,
    "react/prop-types": 0,
    "arrow-parens": "off"
  }
}
